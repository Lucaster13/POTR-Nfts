{
    "type": "module",
    "scripts": {
        "exec-typescript": "ts-node $cmd",
        "pin-potrs": "npm run exec-typescript ./src/pin-potrs.ts",
        "pin-coins": "npm run exec-typescript ./src/pin-coins.ts",
        "pin-all-assets": "npm run pin-coins && npm run pin-potrs",
        "unpin-potrs": "npm run exec-typescript ./src/unpin-potrs.ts",
        "unpin-coins": "npm run exec-typescript ./src/unpin-coins.ts",
        "unpin-all-assets": "npm run unpin-coins && npm run unpin-potrs",
        "mint-potrs": "npm run exec-typescript ./src/mint-potrs.ts",
        "mint-coins": "npm run exec-typescript ./src/mint-coins.ts",
        "mint-all-assets": "npm run mint-coins && npm run mint-potrs",
        "destroy-potrs": "npm run exec-typescript ./src/destroy-potrs.ts",
        "destroy-coins": "npm run exec-typescript ./src/destroy-coins.ts",
        "force-destroy-assets": "npm run exec-typescript ./src/destroy-assets.ts",
        "clean-potrs": "npm run unpin-potrs && npm run destoy-potrs",
        "clean-coins": "npm run unpin-coins && npm run destroy-coins",
        "clean-all-assets": "npm run force-destroy-assets && npm run unpin-all-assets",
        "clean-build": "rm -rf node_modules dist",
        "build": "tsc"
    },
    "dependencies": {
        "@reach-sh/stdlib": "^0.1.11-rc.7",
        "axios": "^1.3.4",
        "bottleneck": "^2.19.5",
        "files-from-path": "^0.2.6",
        "mime": "^3.0.0",
        "multiformats": "^9.7.1",
        "nft.storage": "^7.0.0",
        "node-fetch": "^3.3.0",
        "potr-types": "github:Lucaster13/POTR-Types",
        "url-parse": "^1.5.10"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.18.9",
        "@babel/plugin-syntax-import-assertions": "^7.18.6",
        "@types/node": "^18.7.13",
        "@typescript-eslint/eslint-plugin": "^5.35.1",
        "@typescript-eslint/parser": "^5.35.1",
        "eslint": "^8.21.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-json": "^3.1.0",
        "prettier": "2.7.1",
        "typescript": "^4.8.2"
    }
}
