{
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest",
        "requireConfigFile": false,
        "babelOptions": {
            "plugins": [
                "@babel/plugin-syntax-import-assertions"
            ],
        },
    },
    "parser": "@babel/eslint-parser",
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:json/recommended",
        "airbnb"
    ],

    "rules": {
        "max-len": [1,140 ],
        "indent": [2,4],
        "quotes": [ 1, "double"],
        "import/prefer-default-export": "off",
        "array-bracket-newline": ["warn", { "minItems": 3} ],
        "no-unused-vars": "warn",
        "no-return-assign": 0,
        "no-promise-executor-return": 0,
        "no-return-await": 0,
        "array-bracket-spacing": [1,"always"],
        "no-console": 0,
        "import/extensions": 0,
        "array-element-newline": [1, {"minItems": 3}],
        "no-undef": "off",
        "no-use-before-define": "off",
        "comma-dangle": "off",
        "no-await-in-loop": "off",
        "no-unused-expressions": "off",
        "func-names": "off",
        "no-plusplus": "off",
        "no-restricted-syntax": "off",
        "no-loop-func": "off",
        "no-unreachable": "warn"
    },
    "overrides": [
    ]
}